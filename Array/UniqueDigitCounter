// Write a program to find the count of numbers that consists of unique digits.

public class UniqueDigitCounter {
    public static void main(String[] args) {
        int n = 100;
        int count = 0;
        
        for (int i = 1; i <= n; i++) {
            if (hasUniqueDigits(i)) {
                count++;
            }
        }
        System.out.println("Number of integers with unique digits: " + count);
    }
    public static boolean hasUniqueDigits(int num) {
        int[] digits = new int[10];
        
        while (num > 0) {
            int digit = num % 10;
            if (digits[digit] > 0) {
                return false;
            }
            digits[digit]++;
            num /= 10;
        }
        
        return true;
    }
}
