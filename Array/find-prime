

// write a program to display prime numbers from a given list of integers using java
import java.util.ArrayList;
public class Main {
    public static void main(String[] args) {
        ArrayList<Integer> val =  new ArrayList<Integer>();
        ArrayList<Integer> prime =  new ArrayList<Integer>();
        
        val.add(2);
        val.add(3);
        val.add(13);
        val.add(15);
        val.add(19);
        
        for(int num : val){
            if(isPrime(num))
            prime.add(num);
        }
        // print as a list
        System.out.println(prime);
        
    // print using array
        for(int prm: prime)
        System.out.println(prm);
    }
    // check if prime
   public static boolean isPrime(int n){
       
       for(int i=2;i<Math.sqrt(n);i++){
           if(n%i==0)
           return false;
           else
           return true;
       }
       return false;
   }
}
